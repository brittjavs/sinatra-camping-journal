  <h1> CampTales </h1>
  <h2> <%= current_user.username %>'s Journal</h2>
  <h3> Journal Entries </h3>
  <ul>
  <%@trips.each do |trip|%>
  <li>
  <%=trip.date_camped%>
  <%=trip.park%>
  <%=trip.campground%>
  <%=trip.notes%>
  </li>
  <%end%>
  
  <h3> Camping Bucket List</h3>
  <ul>
  <%@bucketlist.each do |item|%>
  <li>
  <%=item.park%>
  <%=item.campground%>
  <%=item.notes%>
  </li>
  <%end%>
 
</ul>